#
# THISHIDE
#
OBJECT=THISHIDE
THISHIDE:TYPE=BEHAVIOUR
THISHIDE:CODE={THIS^HIDE();}
#
# THISPLAY
#
OBJECT=THISPLAY
THISPLAY:TYPE=BEHAVIOUR
THISPLAY:CODE={THIS^PLAY("PLAY");}
#
# REKSIOSPI
#
OBJECT=REKSIOSPI
REKSIOSPI:TYPE=BEHAVIOUR
REKSIOSPI:CODE={REKSIO^PLAY("SPI");VARLEVEL1^SET("S12_0_WBUDZIE");}
#
# REKSIOBUDZISIE
#
OBJECT=REKSIOBUDZISIE
REKSIOBUDZISIE:TYPE=BEHAVIOUR
REKSIOBUDZISIE:CODE={REKSIO^PLAY("BUDZISIE");CANVASOBSERVER^SETBACKGROUND("BKG2.IMG");}
#
# REKSIOWSTAJE
#
OBJECT=REKSIOWSTAJE
REKSIOWSTAJE:TYPE=BEHAVIOUR
REKSIOWSTAJE:CODE={REKSIO^PLAY("WSTAJE");}
#
# DOS13
#
OBJECT=DOS13
DOS13:TYPE=BEHAVIOUR
DOS13:CODE={PRZYGODA^GOTO( "S13_1_BUDA" );}
#
# S2
#
OBJECT=S2
S2:TYPE=BEHAVIOUR
S2:CODE={NARR12_0_2^PLAY();}
#
# ZINCUJ
#
OBJECT=ZINCUJ
ZINCUJ:TYPE=BEHAVIOUR
ZINCUJ:CODE={VAR_1^INC();}


