#
# THISPLAY
#
OBJECT=THISPLAY
THISPLAY:TYPE=BEHAVIOUR
THISPLAY:CODE={THIS^PLAY();}
#
# THISHIDE
#
OBJECT=THISHIDE
THISHIDE:TYPE=BEHAVIOUR
THISHIDE:CODE={THIS^HIDE();}
#
# REXWCHODZI
#
OBJECT=REXWCHODZI
REXWCHODZI:TYPE=BEHAVIOUR
REXWCHODZI:CODE={REX14_1^PLAY( "ENTRE" );VARLEVEL1^SET("S14_1_PRZEDKOGUTEM");}
#
# KOGENTRE
#
OBJECT=KOGENTRE
KOGENTRE:TYPE=BEHAVIOUR
KOGENTRE:CODE={KOG14_1^PLAY( "ENTRE" );}
#
# KRETENTRE
#
OBJECT=KRETENTRE
KRETENTRE:TYPE=BEHAVIOUR
KRETENTRE:CODE={KRET14_1^PLAY( "WYKOPUJE" );REX14_1^PLAY( "DOKRETA" );E_KOJAK^PLAY();}
#
# KRETNONIEWIERZE
#
OBJECT=KRETNONIEWIERZE
KRETNONIEWIERZE:TYPE=BEHAVIOUR
KRETNONIEWIERZE:CODE={KOPIEC^SHOW();KRET14_1^PLAY( "ENTRE" );KOG14_1^PLAY( "BEZRUCH" );}
