#
# BEH0
#
OBJECT=BEH0
BEH0:TYPE=BEHAVIOUR
BEH0:CODE={KOGUTSEQ^PLAY("BIEGA");VARLEVEL1^SET("S14_0_PRZEDKOGUTEM");}
#
# THISPLAY
#
OBJECT=THISPLAY
THISPLAY:TYPE=BEHAVIOUR
THISPLAY:CODE={THIS^PLAY();}
#
# BEH1
#
OBJECT=BEH1
BEH1:TYPE=BEHAVIOUR
BEH1:CODE={KOGUTSEQ^STOP(FALSE);KOGUTSEQ^PLAY( "GADAWEJSCIE" );REKSIO^SETPRIORITY(2);}
#
# BEH2
#
OBJECT=BEH2
BEH2:TYPE=BEHAVIOUR
BEH2:CODE={BUTKOGUT^ENABLE();KOGUTSEQ^STOP(FALSE);KOGUTSEQ^PLAY( "BEZRUCHZGADANIEM" );REKSIO^PLAY( "ROZKLADA" );DO15^ENABLE();}
#
# DISBUTVIS
#
OBJECT=DISBUTVIS
DISBUTVIS:TYPE=BEHAVIOUR
DISBUTVIS:CODE={THIS^DISABLEBUTVISIBLE();}
#
# KOGUTNAKLIK
#
OBJECT=KOGUTNAKLIK
KOGUTNAKLIK:TYPE=BEHAVIOUR
KOGUTNAKLIK:CODE={KOGUTSEQ^STOP(FALSE);KOGUTSEQ^PLAY("GADAKLIK");BUTKOGUT^DISABLEBUTVISIBLE();}
#
# REKSIODO15
#
OBJECT=REKSIODO15
REKSIODO15:TYPE=BEHAVIOUR
REKSIODO15:CODE={REKSIO^PLAY("ODCHODZI");DO15^DISABLE();}
#
# DOS15
#
OBJECT=DOS15
DOS15:TYPE=BEHAVIOUR
DOS15:CODE={PRZYGODA^GOTO( "S15_0_KURNIK" );}


