OBJECT=__INIT__
__INIT__:TYPE=BEHAVIOUR
__INIT__:CODE={ /
    ARRAYPRZEDMIOTY^REMOVEALL(); /
    ARRAYPRZEDMIOTY^ADD("BUTELKA2"); /
    ARRAYPRZEDMIOTY^ADD("RURAODKURA"); /
    ARRAYPRZEDMIOTY^ADD("SZPULA"); /
    ARRAYPRZEDMIOTY^ADD("LEJEK"); /
    BEHINITSCENE^RUN("STAW"); /
    @IF(CONSTRUCTION_PHASE^GET(), "!_", 6, "DISABLE_WEDKA", ""); /
}

OBJECT=DISABLE_WEDKA
DISABLE_WEDKA:TYPE=BEHAVIOUR
DISABLE_WEDKA:CODE={ /
    ITEM_PICK_LEJEK^SETASBUTTON(FALSE, FALSE); /
}

OBJECT=ON_USE_WEDKA
ON_USE_WEDKA:TYPE=BEHAVIOUR
ON_USE_WEDKA:CODE={ /
    INCREMENT_CONSTRUCTION_PHASE^RUN(); /
}
