IMGPODWORKO:RELEASE=TRUE
IMGPODWORKO:MONITORCOLLISION=FALSE
IMGPODWORKO:MONITORCOLLISIONALPHA=FALSE

OBJECT=__INIT__
__INIT__:TYPE=BEHAVIOUR
__INIT__:CODE={ /
    ARRAYPRZEDMIOTY^REMOVEALL(); /
    ARRAYPRZEDMIOTY^ADD("BUTELKA2"); /
    ARRAYPRZEDMIOTY^ADD("RURAODKURA"); /
    ARRAYPRZEDMIOTY^ADD("SZPULA"); /
    ARRAYPRZEDMIOTY^ADD("LEJEK"); /
    BEHINITSCENE^RUN("STAW"); /
    CALLBACK_TIMER_ROUTINE^SET("START_OUTRO"); /
}

OBJETC=START_OUTRO
START_OUTRO:TYPE=BEHAVIOUR
#START_OUTRO:CONDITION=BUDOWA_WIN_CONDITION
START_OUTRO:CODE={ /
    SHOW_TEXT^RUN("OUTRO"); /
