OBJECT=BEHHIDEANN_ANN_NAME
BEHHIDEANN_ANN_NAME:TYPE=STRING

OBJECT=BEHHIDEANN
BEHHIDEANN:TYPE=BEHAVIOUR
BEHHIDEANN:CODE={ /
    BEHHIDEANN_ANN_NAME^SET(["ANIMO_" + ALLITEMS^GET($1)]); /
    *BEHHIDEANN_ANN_NAME^SETASBUTTON(FALSE, FALSE); /
    *BEHHIDEANN_ANN_NAME^STOP(); /
    *BEHHIDEANN_ANN_NAME^HIDE(); /
}

OBJECT=BEHSHOWANNS_ANN_NAME
BEHSHOWANNS_ANN_NAME:TYPE=STRING

OBJECT=BEHSHOWANNS
BEHSHOWANNS:TYPE=BEHAVIOUR
BEHSHOWANNS:CODE={ /
    BEHSHOWANNS_ANN_NAME^SET(["ANIMO_" + ARRAYPRZEDMIOTY^GET($1)]); /
    *BEHSHOWANNS_ANN_NAME^SETASBUTTON(TRUE, TRUE); /
    *BEHSHOWANNS_ANN_NAME^SHOW(); /
    *BEHSHOWANNS_ANN_NAME^PLAY("PLAY"); /
}

OBJECT=BEHSHOWMENU_ITEM_NAME
BEHSHOWMENU_ITEM_NAME:TYPE=STRING

OBJECT=BEHSHOWMENU_ANN_NAME
BEHSHOWMENU_ANN_NAME:TYPE=STRING

OBJECT=BEHSHOWMENU
BEHSHOWMENU:TYPE=BEHAVIOUR
BEHSHOWMENU:CODE={ /
    BEHSHOWMENU_ITEM_NAME^SET(["ANIMOMENU_" + KIESZEN^GET($1)]); /
    *BEHSHOWMENU_ITEM_NAME^SETPOSITION(0, 0); /
    *BEHSHOWMENU_ITEM_NAME^MOVE([70 * $1], -515); /
    *BEHSHOWMENU_ITEM_NAME^SETASBUTTON(TRUE, TRUE); /
    BEHSHOWMENU_ANN_NAME^SET(["ANIMO_" + KIESZEN^GET($1)]); /
    @IF(ARRAYPRZEDMIOTY^FIND(KIESZEN^GET($1)), "<", 0, "{*BEHSHOWMENU_ANN_NAME^SETASBUTTON(FALSE, FALSE); *BEHSHOWMENU_ANN_NAME^HIDE();}", ""); /
}

OBJECT=BEHHIDEUSEDITEMS_ANN_NAME
BEHHIDEUSEDITEMS_ANN_NAME:TYPE=STRING

OBJECT=BEHHIDEUSEDITEMS
BEHHIDEUSEDITEMS:TYPE=BEHAVIOUR
BEHHIDEUSEDITEMS:CODE={ /
    BEHHIDEUSEDITEMS_ANN_NAME^SET(["ANIMO_" + ZBUDOWANE^GET($1)]); /
    @IF(ARRAYPRZEDMIOTY^FIND(ZBUDOWANE^GET($1)), "<", 0, "{*BEHSHOWMENU_ANN_NAME^SETASBUTTON(FALSE, FALSE); *BEHSHOWMENU_ANN_NAME^HIDE();}", ""); /
}

OBJECT=BEHADDITEMTOMENU_ANN_NAME
BEHADDITEMTOMENU_ANN_NAME:TYPE=STRING

OBJECT=BEHADDITEMTOMENU
BEHADDITEMTOMENU:TYPE=BEHAVIOUR
BEHADDITEMTOMENU:CONDITION=CONDMENUSIZE
BEHADDITEMTOMENU:CODE={ /
    KIESZEN^ADD($1); /
    BEHADDITEMTOMENU_ANN_NAME^SET(["ANIMO_" + $1]); /
    *BEHADDITEMTOMENU_ANN_NAME^SETASBUTTON(FALSE, FALSE); /
    *BEHADDITEMTOMENU_ANN_NAME^HIDE(); /
    ANIMOBALIA^HIDE(); /
    BEHSHOWMENU^RUNLOOPED(0, KIESZEN^GETSIZE()); /
    KIESZEN^SAVEINI(); /
    ZBUDOWANE^SAVEINI(); /
}

OBJECT=BEHREMOVEMENUITEM_ANN_NAME
BEHREMOVEMENUITEM_ANN_NAME:TYPE=STRING

OBJECT=BEHREMOVEMENUITEM_ITEM_NAME
BEHREMOVEMENUITEM_ITEM_NAME:TYPE=STRING

OBJECT=BEHREMOVEMENUITEM
BEHREMOVEMENUITEM:TYPE=BEHAVIOUR
BEHREMOVEMENUITEM:CODE={ /
    BEHREMOVEMENUITEM_ANN_NAME^SET(["ANIMO_" + $1]); /
    *BEHREMOVEMENUITEM_ANN_NAME^SETASBUTTON(TRUE, TRUE); /
    *BEHREMOVEMENUITEM_ANN_NAME^SHOW(); /
    BEHREMOVEMENUITEM_ITEM_NAME^SET(["ANIMOMENU_" + $1]); /
    *BEHREMOVEMENUITEM_ITEM_NAME^SETASBUTTON(FALSE, FALSE); /
    *BEHREMOVEMENUITEM_ITEM_NAME^STOP(FALSE); /
    *BEHREMOVEMENUITEM_ITEM_NAME^HIDE(); /
    KIESZEN^REMOVEAT(KIESZEN^FIND($1)); /
    BEHSHOWMENU^RUNLOOPED(0, KIESZEN^GETSIZE()); /
    KIESZEN^SAVEINI(); /
    ZBUDOWANE^SAVEINI(); /
}

OBJECT=BEHSHOWSHIPPART_PART_NAME
BEHSHOWSHIPPART_PART_NAME:TYPE=STRING

OBJECT=BEHSHOWSHIPPART
BEHSHOWSHIPPART:TYPE=BEHAVIOUR
BEHSHOWSHIPPART:CODE={ /
    BEHSHOWSHIPPART_PART_NAME^SET(["ANIMOBUDOWA_" + ZBUDOWANE^GET($1)]); /
    *BEHSHOWSHIPPART_PART_NAME^PLAY("PLAY"); /
}

OBJECT=BEHSHOWSHIP
BEHSHOWSHIP:TYPE=BEHAVIOUR
BEHSHOWSHIP:CODE={ /
    BEHSHOWSHIPPART^RUNLOOPED(0, ZBUDOWANE^GETSIZE()); /
}

OBJECT=BEHCOUNTITEMS
BEHCOUNTITEMS:TYPE=BEHAVIOUR
BEHCOUNTITEMS:CODE={ /
    VARINTTEMP^ADD(ZBUDOWANE^CONTAINS(ARRAYTEMP^GET($1))); /
}

OBJECT=BEHADDITEMTOSHIP
BEHADDITEMTOSHIP:TYPE=BEHAVIOUR
BEHADDITEMTOSHIP:CODE={ /
    VARINTTEMP^SET(0); /
    VARSTRINGTEMP^SET($1); /
    BEHCOUNTITEMS^RUNLOOPED(0, ARRAYTEMP^GETSIZE()); /
    CONDADDITEMTOSHIP^BREAK(TRUE); /
    ZBUDOWANE^ADD($1); /
    VARSTRINGTEMP^SET(["ANIMOMENU_"+$1]); /
    APPLICATION^RUN( VARSTRINGTEMP, "SETASBUTTON", FALSE, FALSE ); /
    APPLICATION^RUN( VARSTRINGTEMP, "STOP", FALSE ); /
    APPLICATION^RUN( VARSTRINGTEMP, "HIDE" ); /
    KIESZEN^REMOVEAT( KIESZENFIND($1)); /
    BEHSHOWSHIPPART^RUNLOOPED(0, ZBUDOWANE^GETSIZE());
    BEHSHOWMENU^RUNLOOPED(0, KIESZEN^GETSIZE());
    KIESZEN^SAVEINI(); /y
    ZBUDOWANE^SAVEINI(); /
    VARSTRINGTEMP^SET( $1 ); /
    CONDPLAYSUCCESS^CHECK(TRUE); /
}

OBJECT=CONDHIDEITEM
CONDHIDEITEM:TYPE=CONDITION
CONDHIDEITEM:OPERAND1=VARINTTEMP
CONDHIDEITEM:OPERATOR=NOTEQUAL
CONDHIDEITEM:OPERAND2=-1
CONDHIDEITEM:ONRUNTIMESUCCESS={ /
    APPLICATION^RUN( VARSTRINGTEMP, "SETASBUTTON", FALSE, FALSE ); /
    APPLICATION^RUN( VARSTRINGTEMP, "HIDE" ); /
}

OBJECT=CONDADDITEMTOSHIP
CONDADDITEMTOSHIP:TYPE=CONDITION
CONDADDITEMTOSHIP:OPERAND1=ARRAYTEMP^GETSIZE()
CONDADDITEMTOSHIP:OPERATOR=NOTEQUAL
CONDADDITEMTOSHIP:OPERAND2=VARINTTEMP
CONDADDITEMTOSHIP:ONRUNTIMESUCCESS={BEHREMOVEMENUITEM^RUNC( VARSTRINGTEMP ); }

OBJECT=CONDPLAYSUCCESS
CONDPLAYSUCCESS:TYPE=CONDITION
CONDPLAYSUCCESS:OPERAND1=VARSTRINGTEMP
CONDPLAYSUCCESS:OPERATOR=EQUAL
CONDPLAYSUCCESS:OPERAND2="LINA"
CONDPLAYSUCCESS:ONRUNTIMESUCCESS={MOUSE^DISABLE();}
CONDPLAYSUCCESS:ONRUNTIMEFAILED={}

