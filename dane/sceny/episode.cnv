BEHINITSCENE:CODE={ /
    LAST_SCENE_NAME^SET($1); /
    CURRENTLY_IN_CUTSCENE^SET(FALSE); /
    BEHHIDEANN^RUNLOOPED(0, ALLITEMS^GETSIZE()); /
    BEHSHOWANNS^RUNLOOPED(0, ARRAYPRZEDMIOTY^GETSIZE()); /
    KIESZEN^LOADINI(); /
    ZBUDOWANE^LOADINI(); /
    BEHSHOWMENUALL^RUN(); /
    BEHHIDEUSEDITEMS^RUNLOOPED(0, ZBUDOWANE^GETSIZE()); /
    BEHHIDESHIP^RUN(); /
    @IF($1, "_", CONSTRUCTION_PLACE, "BEHSHOWSHIP", ""); /
}

OBJECT=BEHINITCUTSCENE
BEHINITCUTSCENE:TYPE=BEHAVIOUR
BEHINITCUTSCENE:CODE={ /
    CURRENTLY_IN_CUTSCENE^SET(TRUE); /
    ARRAYPRZEDMIOTY^REMOVEALL(); /
    BEHHIDEANN^RUNLOOPED(0, ALLITEMS^GETSIZE()); /
    ZBUDOWANE^LOADINI(); /
    BEHHIDESHIP^RUN(); /
}

