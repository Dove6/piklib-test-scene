    @IF(ARRAYPRZEDMIOTY^CONTAINS(KIESZEN^GET($1)), "_", "TRUE", "{*BEHSHOWMENU_ANN_NAME^SETASBUTTON(FALSE, FALSE); *BEHSHOWMENU_ANN_NAME^HIDE();}", ""); /
