#
# BEHINITREKSIO
#
OBJECT=BEHINITREKSIO
BEHINITREKSIO:TYPE=BEHAVIOUR
BEHINITREKSIO:CODE={ANIMOREKSIO^PLAY("WCHODZI");VARLEVEL2^SET("S15_1_KURNIK");ARRAYPRZEDMIOTY^ADD( "DOPALACZ", "BUTELKA", "WIADRO",  "GRABIE", "MLYNEK", "NOGA", "PRZETYKACZKA", "RAMIE",   "CHOMIK" );ARRAYWARSZTATMENUPRZEDMIOTY^LOADINI();ARRAYWARSZTATPRZEDMIOTY^LOADINI();BEHSHOWMENU^RUNLOOPED( 0, ARRAYWARSZTATMENUPRZEDMIOTY^GETSIZE() );BEHHIDEUSEDITEMS^RUNLOOPED( 0, ARRAYWARSZTATPRZEDMIOTY^GETSIZE() );}
#
# BEHPLAYREKSIO
#
OBJECT=BEHPLAYREKSIO
BEHPLAYREKSIO:TYPE=BEHAVIOUR
BEHPLAYREKSIO:CONDITION=CONDREKSIOSTOI
BEHPLAYREKSIO:CODE={ANIMOREKSIO^PLAY( $1 );}
#
# BEHSHOWMENU
#
OBJECT=BEHSHOWMENU
BEHSHOWMENU:TYPE=BEHAVIOUR
BEHSHOWMENU:CODE={VARSTRINGTEMP^SET( [ "ANIMOMENU_" + ARRAYWARSZTATMENUPRZEDMIOTY^GET( $1 ) ]  );APPLICATION^RUN( VARSTRINGTEMP, "SETPOSITION", 0, 0 );APPLICATION^RUN( VARSTRINGTEMP, "MOVE",  [70*$1],  -515 );APPLICATION^RUN( VARSTRINGTEMP, "SETASBUTTON", TRUE, TRUE );VARINTTEMP^SET( ARRAYPRZEDMIOTY^FIND( ARRAYWARSZTATMENUPRZEDMIOTY^GET( $1 )  ) );VARSTRINGTEMP^SET( [ "ANIMO" + ARRAYWARSZTATMENUPRZEDMIOTY^GET( $1 ) ]  );CONDHIDEITEM^CHECK(TRUE);}
#
# BEHHIDEUSEDITEMS
#
OBJECT=BEHHIDEUSEDITEMS
BEHHIDEUSEDITEMS:TYPE=BEHAVIOUR
BEHHIDEUSEDITEMS:CODE={VARINTTEMP^SET( ARRAYPRZEDMIOTY^FIND( ARRAYWARSZTATPRZEDMIOTY^GET( $1 )  ) );VARSTRINGTEMP^SET( [ "ANIMO" + ARRAYWARSZTATPRZEDMIOTY^GET( $1 ) ]  );CONDHIDEITEM^CHECK(TRUE);}
#
# BEHADDITEMTOMENU
#
OBJECT=BEHADDITEMTOMENU
BEHADDITEMTOMENU:TYPE=BEHAVIOUR
BEHADDITEMTOMENU:CONDITION=CONDMENUSIZE
BEHADDITEMTOMENU:CODE={ARRAYWARSZTATMENUPRZEDMIOTY^ADD( $1 );VARSTRINGTEMP^SET( ["ANIMO" + $1] );APPLICATION^RUN( ["ANIMO" + $1], "SETASBUTTON", FALSE, FALSE );APPLICATION^RUN( ["ANIMO" + $1], "HIDE" );BEHSHOWMENU^RUNLOOPED( 0, ARRAYWARSZTATMENUPRZEDMIOTY^GETSIZE() );ANIMOREKSIO^PLAY( RANDOM^GET(0,2) );ARRAYWARSZTATMENUPRZEDMIOTY^SAVEINI();ARRAYWARSZTATPRZEDMIOTY^SAVEINI();}
#
# BEHREMOVEMENUITEM
#
OBJECT=BEHREMOVEMENUITEM
BEHREMOVEMENUITEM:TYPE=BEHAVIOUR
BEHREMOVEMENUITEM:CODE={VARSTRINGTEMP^SET(  ["ANIMO"+$1] );APPLICATION^RUN( VARSTRINGTEMP, "SETASBUTTON", TRUE, TRUE );APPLICATION^RUN( VARSTRINGTEMP, "SHOW" );VARSTRINGTEMP^SET(  ["ANIMOMENU_"+$1] );APPLICATION^RUN( VARSTRINGTEMP, "SETASBUTTON", FALSE, FALSE );APPLICATION^RUN( VARSTRINGTEMP, "HIDE" );ARRAYWARSZTATMENUPRZEDMIOTY^REMOVEAT(ARRAYWARSZTATMENUPRZEDMIOTY^FIND( $1) );BEHSHOWMENU^RUNLOOPED( 0, ARRAYWARSZTATMENUPRZEDMIOTY^GETSIZE() );ANIMOREKSIO^PLAY( RANDOM^GET(0,2) );ARRAYWARSZTATMENUPRZEDMIOTY^SAVEINI();ARRAYWARSZTATPRZEDMIOTY^SAVEINI();}


